{% extends 'OpenOrchestraDisplayBundle:Block:layout.html.twig' %}

{% block content %}
    <div class="configurable-content">
        <h2>{{ content.name }}</h2>
        {% for attribute in content.attributes %}
            <p><b>{{ attribute.name }}:</b> {{ attribute.stringValue }}</p>
        {% endfor %}
    </div>
    <script>
        require(['{{ asset('/bundles/openorchestradisplay/block/ConfigurableContent/blockLoader.js') }}']);
    </script>
{% endblock %}
